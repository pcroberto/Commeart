    <cache name="buscaGruposPeloIdFamilia" maxElementsInMemory="50" timeToLiveSeconds="1000" />
    <cache name="buscaFornecedorPeloId" maxElementsInMemory="50" timeToLiveSeconds="500" />
    <cache name="buscaEmpresas" maxElementsInMemory="50" timeToLiveSeconds="1000" />
    <cache name="buscarMetaHeaderPorIdProduto" maxElementsInMemory="50" timeToLiveSeconds="1000" />

</ehcache>
